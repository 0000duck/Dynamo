<UserControl x:Class="Dynamo.Wpf.Controls.DelcamViewerControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:powermill="clr-namespace:Delcam.WPFControls.ProductControls.PowerMILL;assembly=Delcam.WPFControls.ProductControls.PowerMILL"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="600">
    
    <Grid Width="Auto" Height="Auto">
        <powermill:DelcamPowerMILLBase Automation="{Binding PowerMillAutomation}"
                                       ToolbarVisibility="Collapsed" />
    </Grid>
</UserControl>
